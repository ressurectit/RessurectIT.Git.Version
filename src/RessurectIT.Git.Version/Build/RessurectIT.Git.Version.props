<?xml version="1.0" encoding="utf-8"?>
<Project>
    <PropertyGroup>
        <BuildVersionPath>buildVersion.txt</BuildVersionPath>
        <GitArgBranchName></GitArgBranchName>
        <GitArgBuildNumber></GitArgBuildNumber>
        <GitArgTagPrefix></GitArgTagPrefix>
        <GitArgIgnoreBranchPrefix></GitArgIgnoreBranchPrefix>
        <GitArgPre></GitArgPre>
        <GitArgSuffix></GitArgSuffix>
        <GitArgCurrentVersion></GitArgCurrentVersion>
        <GitArgWorkingDirectory></GitArgWorkingDirectory>
        <GitArgConfigPath></GitArgConfigPath>
        <GitArgNoIncrement></GitArgNoIncrement>
        <GitVersionToolWorkingDir Condition="'$(OS)' == 'Windows_NT'">$(MSBuildThisFileDirectory)../tools/win/</GitVersionToolWorkingDir>
        <GitVersionToolWorkingDir Condition="'$(OS)' != 'Windows_NT'">$(MSBuildThisFileDirectory)../tools/dotnet/</GitVersionToolWorkingDir>
        <GitVersionTool Condition="'$(OS)' == 'Windows_NT'">RessurectIT.Git.Version.exe</GitVersionTool>
        <GitVersionTool Condition="'$(OS)' != 'Windows_NT'">RessurectIT.Git.Version.dll</GitVersionTool>
        <NodeJsFolders>$(NuGetPackageFolders)</NodeJsFolders>
        <NodeJsPath Condition="'$(OS)' == 'Windows_NT'">node.js.redist.win/22.13.1/tools/x64/node.exe</NodeJsPath>
        <NodeJsPath Condition="'$(OS)' != 'Windows_NT'">node.js.redist.linux/22.13.1/tools/x64/node</NodeJsPath>
    </PropertyGroup>
</Project>